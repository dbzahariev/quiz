{
  "name": "instaquiz",
  "version": "1.0.0",
  "description": "A fun and exciting quiz application built with ReactJS, express, and MongoDB.",
  "main": "server.js",
  "scripts": {
    "client": "npm run start --prefix client",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "test": "SET \"NODE_ENV=test\" && mocha tests/**/*.test.js",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "keywords": [],
  "author": "Uzoanya Dominic",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.9",
    "morgan": "^1.10.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "18.12.0",
    "npm": "8.19.2"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbzahariev/quiz.git"
  },
  "bugs": {
    "url": "https://github.com/dbzahariev/quiz/issues"
  },
  "homepage": "https://github.com/dbzahariev/quiz#readme"
}
